{
    "name": "devsen/gateway-api",
    "description": "Gateway API per lettura dati MySQL",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "vlucas/phpdotenv": "^5.5",
        "monolog/monolog": "^3.4",
        "symfony/http-foundation": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "DevSen\\Gateway\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}